From 303be2db79eb4849cba954f6d7b0c97ba5132da3 Mon Sep 17 00:00:00 2001
From: Jari Aalto <jari.aalto@cante.net>
Date: Thu, 4 Mar 2010 19:31:21 +0200
Subject: [PATCH] Makefile: (clean): do not delete original files

Signed-off-by: Jari Aalto <jari.aalto@cante.net>
---
 Makefile |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/Makefile b/Makefile
index 388bf09..f7e7159 100644
--- a/Makefile
+++ b/Makefile
@@ -26,7 +26,7 @@ wiggle.man : wiggle.1
 	nroff -man wiggle.1 > wiggle.man
 
 clean:
-	rm -f *.o *.man wiggle .version* version
+	rm -f *.o *.man wiggle .version*
 	find . -name core -o -name '*.tmp*' -o -name .tmp | xargs rm -f
 
 install : wiggle wiggle.1
